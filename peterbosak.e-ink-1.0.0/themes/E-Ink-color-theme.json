  "tokenColors": [
    {
      "name": "Function",
      "scope": "support.function, variable.function",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "functions",
      "scope": "entity.name.function, storage.type",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Call",
      "scope": "meta.function-call entity.name.function",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "condifionals and cycles",
      "scope": ["keyword.control.conditional"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable Interpolation",
      "scope": "variable.interpolation",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
        "name": "Variable Parameter",
        "scope": "variable.parameter",
        "settings": {
          "fontStyle": "bold"
        }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Key",
      "scope": "constant.other.object.key string.unquoted",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope":
        "string.quoted, string, punctuation.tag.definition.string, tag.string",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "this",
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Key",
      "scope": "constant.other.object.key string.unquoted",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#888888"
      }
    },
    {
      "name": "Entity Name Type",
      "scope": "entity.name.type",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
        "name": "Entity Other Inherited Class",
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": "bold"
        }
    },
    {
      "name": "Keyword",
      "scope": "keyword, keyword.operator.new, keyword.other, keyword.control",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword (Operators)",
      "scope": "keyword.operator",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword (Conditionals)",
      "scope": "keyword.control.conditional, source.php keyword.control",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword (TryCatch)",
      "scope": "keyword.control.trycatch",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Control",
      "scope": "keyword.control",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword (Flow e.g. return, await)",
      "scope": "keyword.control.flow",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Braces",
      "scope": "meta.brace, punctuation.definition.parameters.begin, punctuation.definition.parameters.end,\n\t\t\tpunctuation.definition.block, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end,\n\t\t\tpunctuation.section.embedded.begin, punctuation.section.embedded.end",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Braces (Square)",
      "scope": "meta.brace.square",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000",
      }
    },
    {
      "name": "[JSON] - Property Names",
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "SCSS Variable",
      "scope": [
        "variable.scss",
        "variable.sass",
        "variable.parameter.url.scss",
        "variable.parameter.url.sass"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Variables in SASS At-Rules",
      "scope": [
        "source.css.scss meta.at-rule variable",
        "source.css.sass meta.at-rule variable"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Variables in SASS At-Rules",
      "scope": [
        "source.css.scss meta.at-rule variable",
        "source.css.sass meta.at-rule variable"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Attribute Name for SASS",
      "scope": [
        "meta.attribute-selector.scss entity.other.attribute-name.attribute",
        "meta.attribute-selector.sass entity.other.attribute-name.attribute"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Tag names in SASS",
      "scope": ["entity.name.tag.scss", "entity.name.tag.sass"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "SASS Keyword Other Unit",
      "scope": ["keyword.other.unit.scss", "keyword.other.unit.sass"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted",
        "punctuation.definition.inserted.diff",
        "punctuation.definition.to-file.diff",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#888888",
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted",
        "punctuation.definition.deleted.diff",
        "punctuation.definition.from-file.diff",  
        "meta.diff.header.from-file"
      ],
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#bbbbbb"
      }
    },
  ]
